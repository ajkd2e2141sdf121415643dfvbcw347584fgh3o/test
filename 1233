loadstring(game:HttpGet('https://raw.githubusercontent.com/7878wqz/sc1/refs/heads/main/fatbbff2'))()
page1:Seperator("🏠")
page1:Toggle("AutoFarmLevel\nออโต้ฟาร์มเลเวล", _G.Settings.AutoFarm, function(value)
    _G.AutoFarm = value
    _G.Settings.AutoFarm = value
    SaveSettings()
    if value == false then
        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
    end
end)  

AttackRandomType_MonCFrame = 1
spawn(function()
    while wait() do 
        AttackRandomType_MonCFrame = math.random(1,5)
        wait(0.2)
    end
end)

_G.Auto_CFrame = true
spawn(function()
    while task.wait(0.1) do
        if _G.AutoFarm and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and LocalPlayer.Character.Humanoid.Health > 0 then
            pcall(function()
                local questData = CheckQuest()
                if not questData or not questData.NameQuest or not questData.CFrameQuest or not questData.CFramemonenemyspawn then
                    warn("Quest data invalid, retrying...")
                    return
                end
                
                local QuestGui = LocalPlayer.PlayerGui.Main.Quest
                local QuestVisible = QuestGui.Visible
                local QuestTitle = QuestVisible and QuestGui.Container.QuestTitle.Title.Text or ""
                
                if QuestVisible and not string.find(QuestTitle, questData.NameMon, 1, true) then
                    warn("Abandoning quest: ", QuestTitle)
                    ReplicatedStorage.Remotes.CommF_:InvokeServer("AbandonQuest")
                    wait(0.5)
                end
                
                if not QuestVisible then
                    toTarget(questData.CFrameQuest)
                    local distance = (LocalPlayer.Character.HumanoidRootPart.Position - questData.CFrameQuest.Position).Magnitude
                    if distance <= 10 then
                        warn("Starting quest: ", questData.NameQuest, questData.LevelQuest)
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("StartQuest", questData.NameQuest, questData.LevelQuest)
                        wait(0.5)
                    end
                else
                    local foundMob = nil
                    for _, v in pairs(Workspace.Enemies:GetChildren()) do
                        if v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            if string.find(v.Name, questData.NameMon, 1, true) then
                                foundMob = v
                                saveMonsterCFrame(v)
                                break
                            end
                        end
                    end
                    
                    if foundMob then
                        warn("Found mob: ", foundMob.Name)
                        repeat task.wait(0.1)
                            _G.AutoHaki = true
                            EquipWeapon(_G.SelectWeapon)
                            _G.BringMob = true
                            PosMon = foundMob.HumanoidRootPart.CFrame
                            foundMob.HumanoidRootPart.Size = Vector3.new(70, 70, 70)
                            foundMob.HumanoidRootPart.CanCollide = false
                            --foundMob.Humanoid.WalkSpeed = 0
                            foundMob.Head.CanCollide = false
                            toTarget(foundMob.HumanoidRootPart.CFrame * MethodFarm)
                        until not _G.AutoFarm or not foundMob.Parent or foundMob.Humanoid.Health <= 0 or not QuestGui.Visible
                        _G.BringMob = false
                        if foundMob.Humanoid.Health <= 0 then
                            foundMob:Destroy()
                        end
                    else
                        local foundSpawn = false
                        for _, v in pairs(Workspace.EnemySpawns:GetChildren()) do
                            if v:IsA("Part") and string.find(v.Name, questData.NameMon, 1, true) then
                                warn("Moving to spawn: ", v.Name)
                                toTarget(v.CFrame * MethodFarm)
                                foundSpawn = true
                                break
                            end
                        end
                        if not foundSpawn then
                            if _G.Auto_CFrame then
                                local targetCFrame = handleAutoCFrame(questData.CFramemonenemyspawn, questData.NameMon)
                                if targetCFrame then
                                    toTarget(targetCFrame * MethodFarm)
                                    if (targetCFrame.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 then
                                        if not SetCFrame or SetCFrame >= #questData.CFramemonenemyspawn then
                                            SetCFrame = 1
                                        else
                                            SetCFrame = SetCFrame + 1
                                        end
                                        wait(0.5)
                                    end
                                else
                                    toTarget(questData.CFrameQuest * MethodFarm)
                                end
                            else
                                local offset
                                if AttackRandomType_MonCFrame == 1 then
                                    offset = CFrame.new(0, 30, 20)
                                elseif AttackRandomType_MonCFrame == 2 then
                                    offset = CFrame.new(0, 30, -20)
                                elseif AttackRandomType_MonCFrame == 3 then
                                    offset = CFrame.new(20, 30, 0)
                                elseif AttackRandomType_MonCFrame == 4 then
                                    offset = CFrame.new(0, 30, -20)
                                elseif AttackRandomType_MonCFrame == 5 then
                                    offset = CFrame.new(-20, 30, 0)
                                end
                                local targetCFrame = (questData.CFramemonenemyspawn[1] or questData.CFrameQuest) * offset
                                warn("Moving to CFramemonenemyspawn with offset: ", targetCFrame)
                                toTarget(targetCFrame)
                            end
                        end
                    end
                end
            end)
        else
            warn("Waiting for character to load or respawn")
            task.wait(1)
        end
    end
end)

