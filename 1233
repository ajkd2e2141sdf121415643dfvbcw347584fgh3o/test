        then
                for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = false    
                    end
                end
            end
        end)
    end)
end)

-- [BringMon]

function InMyNetWork(object)
	if isnetworkowner then
		return isnetworkowner(object)
	else
		if (object.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= _G.BringMode then 
			return true
		end
		return false
	end
end

if game.Players.LocalPlayer.Character:FindFirstChild("Stun") then
	game.Players.LocalPlayer.Character.Stun.Changed:connect(function()
		pcall(function()
			if game.Players.LocalPlayer.Character:FindFirstChild("Stun") then
				game.Players.LocalPlayer.Character.Stun.Value = 0
			end
		end)
	end)
end

function DisabledDamage()
    task.spawn(function()
        game:GetService('RunService').Stepped:Connect(function()
            pcall(function()
                if _G.Disdamage then
                    game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
                else
                    game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = true
                end
            end)
        end)
    end)
    spawn(function()
        game:GetService('RunService').Stepped:Connect(function()
            for i, v in pairs(game:GetService("Workspace")["_WorldOrigin"]:GetChildren()) do
                pcall(function()
                    if v.Name == "CurvedRing" or v.Name == "SlashHit" or v.Name == "SwordSlash" then
                        v:Destroy()
                    end
                end)
            end
        end)
    end)
    spawn(function()
        game:GetService('RunService').Stepped:Connect(function()
                for i, v in pairs(game:GetService("ReplicatedStorage").Effect.Container:GetChildren()) do
                    pcall(function()
                        if v.Name == "Death" then
                        v:Destroy() 
                    end
                end)
            end
        end)
    end)
end

local tween 

local function GetIsLand(...)
    local RealtargetPos = { ... }
    local targetPos = RealtargetPos[1]
    local RealTarget
    
    if type(targetPos) == "vector" then
        RealTarget = targetPos
    elseif type(targetPos) == "userdata" then
        RealTarget = targetPos.Position
    elseif type(targetPos) == "number" then
        RealTarget = CFrame.new(unpack(RealtargetPos))
        RealTarget = RealTarget.Position
    end

    local ReturnValue
    local CheckInOut = math.huge
    
    if game.Players.LocalPlayer.Team then
        local teamSpawns = game.Workspace._WorldOrigin.PlayerSpawns:FindFirstChild(tostring(game.Players.LocalPlayer.Team))
        if teamSpawns then
            for i, v in pairs(teamSpawns:GetChildren()) do
                local ReMagnitude = (RealTarget - v:GetModelCFrame().Position).Magnitude 
                if ReMagnitude < CheckInOut then
                    CheckInOut = ReMagnitude
                    ReturnValue = v.Name
                end
            end
        end
        if ReturnValue then
            return ReturnValue
        end
    end
end


local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local currentTween = nil
local currentConn = nil
local lifted = false           
local LIFT_HEIGHT = 180      
local NEAR_LIFT_THRESHOLD = 190
local STOP_THRESHOLD = 1

local function safeGetCharacter()
    local plr = Players.LocalPlayer
    return plr and (plr.Character or plr.CharacterAdded:Wait())
end

local function cancelCurrent()
    if currentTween and currentTween.PlaybackState == Enum.PlaybackState.Playing then
        pcall(function() currentTween:Cancel() end)
    end
    if currentConn then
        pcall(function() currentConn:Disconnect() end)
        currentConn = nil
    end
end

local function toTarget(...)
    local args = { ... }
    local t = args[1]
    local targetVec
    if typeof(t) == "Vector3" then
        targetVec = t
    elseif typeof(t) == "CFrame" then
        targetVec = t.Position
    elseif type(t) == "number" then
        local x,y,z = unpack(args)
        if type(x) == "number" and type(y) == "number" and type(z) == "number" then
            targetVec = Vector3.new(x,y,z)
        else
            return nil, "invalid numeric args"
        end
    else
        return nil, "unsupported target type"
    end

    local char = safeGetCharacter()
    if not char then return nil, "no character" end
    local humanoid = char:WaitForChild("Humanoid")
    local hrp = char:WaitForChild("HumanoidRootPart")

    if humanoid.Health <= 0 then
        cancelCurrent()
        repeat task.wait() until humanoid.Health > 0
        task.wait(0.2)
    end

    local currentPos = hrp.Position
    local toRealDist = (currentPos - targetVec).Magnitude

    if toRealDist > NEAR_LIFT_THRESHOLD and not lifted then
        hrp.CFrame = hrp.CFrame * CFrame.new(0, LIFT_HEIGHT, 0)
        lifted = true
    end

    local tweenTargetVec = targetVec + Vector3.new(0, (lifted and LIFT_HEIGHT or 0), 0)
    local tweenTargetCFrame = CFrame.new(tweenTargetVec)
    local travelDist = (hrp.Position - tweenTargetVec).Magnitude
    local speed
    if travelDist < 200 then
        speed = 800
    elseif travelDist < 500 then
        speed = 350
    elseif travelDist < 750 then
        speed = 300
    else
        speed = 250
    end
    local duration = math.max(0.05, travelDist / speed)
    local info = TweenInfo.new(duration, Enum.EasingStyle.Linear)

    cancelCurrent()

    local ok, err = pcall(function()
        currentTween = TweenService:Create(hrp, info, { CFrame = tweenTargetCFrame })
        currentTween:Play()
    end)
    if not ok then
        warn("Tween creation failed:", err)
        return nil, err
    end

    currentConn = RunService.Heartbeat:Connect(function()
        if not currentTween then return end
        local nowPos = hrp.Position
        local distToReal = (nowPos - targetVec).Magnitude

        if lifted and distToReal <= NEAR_LIFT_THRESHOLD then
            pcall(function() currentTween:Cancel() end)

            local downDist = (hrp.Position - targetVec).Magnitude
            local downDuration = math.max(0.05, downDist / (speed * 2)) 
            local downInfo = TweenInfo.new(downDuration, Enum.EasingStyle.Linear)
            lifted = false
            currentTween = TweenService:Create(hrp, downInfo, { CFrame = CFrame.new(targetVec) })
            currentTween:Play()
            return
        end

        if not lifted and distToReal <= STOP_THRESHOLD then
            pcall(function() currentTween:Cancel() end)
            if currentConn then
                pcall(function() currentConn:Disconnect() end)
                currentConn = nil
            end
            return
        end

        if lifted then
            local distToTweenTarget = (nowPos - tweenTargetVec).Magnitude
            if distToTweenTarget <= STOP_THRESHOLD then
                pcall(function() currentTween:Cancel() end)
            end
        end
    end)

    local tweenfunc = {}

    function tweenfunc:Stop()
        cancelCurrent()
        lifted = false
    end

    function tweenfunc:Wait()
        if currentTween then
            pcall(function() currentTween.Completed:Wait() end)
        end
    end

    return tweenfunc
end


function GetDistance(target)
	return math.floor((target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
end

function GetFightingStyle(Style)
ReturnText = ""
for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
if v:IsA("Tool") then
    if v.ToolTip == Style then
        ReturnText = v.Name
    end
end
end
for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if v:IsA("Tool") then
    if v.ToolTip == Style then
        ReturnText = v.Name
    end
end
end
if ReturnText ~= "" then
return ReturnText
else
return "Not Have"
end
end


