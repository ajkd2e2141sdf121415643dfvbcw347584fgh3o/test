                Mon = "Royal Squad [Lv. 525]",
                LevelQuest = 1,
                NameQuest = "SkyExp2Quest",
                NameMon = "Royal Squad",
                CFrameQuest = CFrame.new(-7906.81592, 5634.6626, -1411.99194),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Royal Squad")
            }
        elseif MyLevel >= 550 and MyLevel <= 624 then
            questData = {
                Mon = "Royal Soldier [Lv. 550]",
                LevelQuest = 2,
                NameQuest = "SkyExp2Quest",
                NameMon = "Royal Soldier",
                CFrameQuest = CFrame.new(-7906.81592, 5634.6626, -1411.99194),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Royal Soldier")
            }
        elseif MyLevel >= 625 and MyLevel <= 649 then
            questData = {
                Mon = "Galley Pirate [Lv. 625]",
                LevelQuest = 1,
                NameQuest = "FountainQuest",
                NameMon = "Galley Pirate",
                CFrameQuest = CFrame.new(5259.81982, 37.3500175, 4050.0293),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Galley Pirate")
            }
        elseif MyLevel >= 650 and MyLevel <= 699 then
            questData = {
                Mon = "Galley Captain [Lv. 650]",
                LevelQuest = 2,
                NameQuest = "FountainQuest",
                NameMon = "Galley Captain",
                CFrameQuest = CFrame.new(5259.81982, 37.3500175, 4050.0293),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Galley Captain")
            }
        end
    
    cachedQuestData = questData
    lastQuestCheck = currentTime
    return questData
end

local EnemySpawns = Instance.new("Folder", Workspace)
EnemySpawns.Name = "EnemySpawns"

local function initializeEnemySpawns()
    if Workspace._WorldOrigin.EnemySpawns then
        for _, v in pairs(Workspace._WorldOrigin.EnemySpawns:GetChildren()) do
            if v:IsA("Part") then
                local spawnPart = v:Clone()
                spawnPart.Name = cleanMobName(v.Name)
                spawnPart.Parent = EnemySpawns
                spawnPart.Anchored = true
            end
        end
    end
    for _, v in pairs(Workspace.Enemies:GetChildren()) do
        if v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") then
            saveMonsterCFrame(v)
            local spawnPart = v.HumanoidRootPart:Clone()
            spawnPart.Name = cleanMobName(v.Name)
            spawnPart.Parent = EnemySpawns
            spawnPart.Anchored = true
        end
    end
    for _, v in pairs(ReplicatedStorage:GetChildren()) do
        if v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") then
            saveMonsterCFrame(v)
            local spawnPart = v.HumanoidRootPart:Clone()
            spawnPart.Name = cleanMobName(v.Name)
            spawnPart.Parent = EnemySpawns
            spawnPart.Anchored = true
        end
    end
end

-- เรียกใช้การเริ่มต้น EnemySpawns
initializeEnemySpawns()
