    World3 = true
end

-- ฟังก์ชันทำความสะอาดชื่อมอนสเตอร์
local function cleanMobName(name)
    return name:gsub("Lv%. ", ""):gsub("[%[%]]", ""):gsub("%d+", ""):gsub("%s+", "")
end

-- ฟังก์ชันหา CFramemonenemyspawn
local function GetCFrameMonEnemySpawn(MobName)
    local matchingCFrames = {}
    local result = string.gsub(MobName, "Lv%. ", "")
    local result2 = string.gsub(result, "[%[%]]", "")
    local result3 = string.gsub(result2, "%d+", "")
    local result4 = string.gsub(result3, "%s+", "")
    
    for _, v in pairs(Workspace.EnemySpawns:GetChildren()) do
        if v.Name == result4 then
            table.insert(matchingCFrames, v.CFrame)
        end
    end
    
    -- เพิ่มพิกัดจาก OldMonsterCFrames
    local cleanName = cleanMobName(MobName)
    if OldMonsterCFrames[cleanName] then
        for _, cframe in ipairs(OldMonsterCFrames[cleanName]) do
            table.insert(matchingCFrames, cframe)
        end
    end
    
    -- พิกัดสำรองสำหรับทุกโลก
    if #matchingCFrames == 0 then
