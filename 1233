        table.insert(matchingCFrames, defaultCFrames[cleanName] or defaultCFrames["default"])
        warn("No spawn points for " .. cleanName .. ", using default CFrame")
    end
    
    return matchingCFrames
end

local function saveMonsterCFrame(monster)
    if monster:IsA("Model") and monster:FindFirstChild("HumanoidRootPart") then
        local cleanName = cleanMobName(monster.Name)
        OldMonsterCFrames[cleanName] = OldMonsterCFrames[cleanName] or {}
        if #OldMonsterCFrames[cleanName] < AutoCFrameSettings.MaxPositions then
            table.insert(OldMonsterCFrames[cleanName], monster.HumanoidRootPart.CFrame)
        end
    end
end
-- ฟังก์ชันตรวจสอบเควสต์
local function CheckQuest()
    local currentTime = tick()
    if cachedQuestData and (currentTime - lastQuestCheck) < QUEST_CACHE_DURATION then
        return cachedQuestData
    end
    
    local MyLevel = LocalPlayer.Data.Level.Value
    local questData = { Mon = nil, LevelQuest = nil, NameQuest = nil, NameMon = nil, CFrameQuest = nil, CFramemonenemyspawn = {} }
        -- World 1 (First Sea)
        if MyLevel >= 0 and MyLevel <= 9 then
            questData = {
                Mon = "Bandit [Lv. 5]",
                LevelQuest = 1,
                NameQuest = "BanditQuest1",
                NameMon = "Bandit",
                CFrameQuest = CFrame.new(1059.97168, 16.4415283, 1547.3728),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Bandit")
            }
        elseif MyLevel >= 10 and MyLevel <= 14 then
            questData = {
                Mon = "Monkey [Lv. 14]",
                LevelQuest = 1,
                NameQuest = "JungleQuest",
                NameMon = "Monkey",
                CFrameQuest = CFrame.new(-1604.12012, 36.8521118, 154.23732),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Monkey")
            }
        elseif MyLevel >= 15 and MyLevel <= 29 then
            questData = {
                Mon = "Gorilla [Lv. 20]",
                LevelQuest = 2,
                NameQuest = "JungleQuest",
                NameMon = "Gorilla",
                CFrameQuest = CFrame.new(-1604.12012, 36.8521118, 154.23732),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Gorilla")
            }
        elseif MyLevel >= 30 and MyLevel <= 39 then
            questData = {
                Mon = "Pirate [Lv. 35]",
                LevelQuest = 1,
                NameQuest = "BuggyQuest1",
                NameMon = "Pirate",
                CFrameQuest = CFrame.new(-1141.07483, 4.10001802, 3831.5498),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Pirate")
            }
        elseif MyLevel >= 40 and MyLevel <= 59 then
            questData = {
                Mon = "Brute [Lv. 45]",
                LevelQuest = 2,
                NameQuest = "BuggyQuest1",
                NameMon = "Brute",
                CFrameQuest = CFrame.new(-1141.07483, 4.10001802, 3831.5498),
                CFramemonenemyspawn = GetCFrameMonEnemySpawn("Brute")
            }
        elseif MyLevel >= 60 and MyLevel <= 74 then
            questData = {
                Mon = "Desert Bandit [Lv. 60]",
                LevelQuest = 1,
                NameQuest = "DesertQuest",
                NameMon = "Desert Bandit",
